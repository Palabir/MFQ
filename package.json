    "@adonisjs/fold": "^4.0.5",
    "@adonisjs/framework": "^4.0.27",
    "@adonisjs/ignitor": "^1.0.14",
    "@adonisjs/lucid": "^4.0.22",
    "@adonisjs/session": "^1.0.19",
    "@adonisjs/shield": "^1.0.4",
    "@nuxtjs/axios": "^5.0.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "material-design-icons": "^3.0.1",
    "node-sass": "^4.9.3",
    "nuxt": "^1.0.0",
    "sass-loader": "^7.1.0",
    "vue-style-loader": "^4.1.2",
    "vuetify": "^1.0.19"
